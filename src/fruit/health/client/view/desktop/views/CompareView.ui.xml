<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
	ui:generateLocales="default" xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style>
	</ui:style>

	<g:HTMLPanel>
		<div ui:field="plansPara">
			<div
				style="float: left; width:45%; min-width: 300px; margin-right: 30px; padding-top: 20px; margin-bottom: 20px;">
				<div ui:field="slider" />
			</div>
			<g:SimplePanel ui:field="chartHolder" />
		</div>
		<p style="float: none;">
			<div style="text-align:center;">
				<g:Anchor ui:field="enterPlan" styleName="button--heroku">
					Click here to add
					a plan.
				</g:Anchor>
			</div>
		</p>

		<div style="float: none;">
			<div style="text-align:center;" class="form-group input-box">
				Share this link:
				<input ui:field="linkToPage" class="form-control  form-contorl-faded  input-sm"
					style="display:inline; width: 40em;" readOnly="true" />
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 