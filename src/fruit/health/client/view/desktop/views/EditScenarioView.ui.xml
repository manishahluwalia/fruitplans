<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
	ui:generateLocales="default" xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style>

	</ui:style>

	<g:HTMLPanel>
		Gender:
		&nbsp; &nbsp; &nbsp;
		<div class="form-group">
			<g:ListBox styleName="input-lg" ui:field="gender">
				<g:item value="male">Male</g:item>
				<g:item value="female">Female</g:item>
			</g:ListBox>
		</div>

		Age:
		&nbsp; &nbsp; &nbsp;
		<div class="form-group has-icon input-box">
			<i class="icon-number form-control-icon  icon-white"></i>
			<input ui:field="age" type="text" placeholder="Age" autofocus=""
				required="" class="form-control  form-contorl-faded  input-lg" name="age" />
		</div>

		Number of Doctor visits:
		&nbsp; &nbsp; &nbsp;
		<div class="form-group has-icon input-box">
			<i class="icon-number form-control-icon  icon-white"></i>
			<input ui:field="numDocVists" type="text"
				placeholder="Number of times you go to the doctor for non-preventive issues"
				autofocus="" required="" class="form-control  form-contorl-faded  input-lg"
				name="numDocVisits" />
		</div>

		Number of Prescriptions:
		&nbsp; &nbsp; &nbsp;
		<div class="form-group has-icon input-box">
			<i class="icon-number form-control-icon  icon-white"></i>
			<input ui:field="numRxs" type="text"
				placeholder="Number of prescription medications you take" autofocus=""
				required="" class="form-control  form-contorl-faded  input-lg" name="numRxs" />
		</div>

		Number of nights in the hospital:
		&nbsp; &nbsp; &nbsp;
		<div class="form-group has-icon input-box">
			<i class="icon-number form-control-icon  icon-white"></i>
			<input ui:field="numHospiDays" type="text"
				placeholder="Number days you spend admitted to a hospital"
				autofocus="" required="" class="form-control  form-contorl-faded  input-lg"
				name="numHospiDays" />
		</div>

		<p>
			<g:Button ui:field="done" styleName="button--heroku">Done</g:Button>
		</p>
	</g:HTMLPanel>
</ui:UiBinder> 